# Example environment variables for koishi-app

# OneBot adapter
ONEBOT_SELF_ID=YOUR_BOT_ID
ONEBOT_TOKEN=YOUR_BOT_TOKEN

# Koishi console admin account
KOISHI_CONSOLE_ADMIN=admin
KOISHI_CONSOLE_PASSWORD=strong_password

# Gemini adapter
GEMINI_API_KEY=AIzaYourKeyHere

# Steam Status Subscribe plugin
STEAM_STATUS_KEY=your_steam_api_key

# Fish Audio plugin
FISH_AUDIO_API_KEY=your_fish_audio_api_key 